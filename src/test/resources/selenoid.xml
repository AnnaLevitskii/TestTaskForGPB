<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="selenoid" verbose="1">
    <test name="selenoid_chrome">
        <parameter name="browser" value="chrome_selenoid"/>
        <classes>
            <class name="com.ui.LoginTest"/>
            <class name="com.ui.AddContactTest"/>
            <class name="com.ui.RemoveContactTest"/>
        </classes>
    </test>
    <test name="selenoid_edge">
        <parameter name="browser" value="edge_selenoid"/>
        <classes>
            <class name="com.ui.LoginTest"/>
            <class name="com.ui.AddContactTest"/>
            <class name="com.ui.RemoveContactTest"/>
        </classes>
    </test>
    <test name="selenoid_firefox">
        <parameter name="browser" value="firefox_selenoid"/>
        <classes>
            <class name="com.ui.LoginTest"/>
            <class name="com.ui.AddContactTest"/>
            <class name="com.ui.RemoveContactTest"/>
        </classes>
    </test>
</suite>